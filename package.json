{
  "name": "server",
  "version": "0.0.1",
  "description": "PDI API",
  "author": {
    "name": "Yan Gon√ßalves",
    "email": "fgyan94@gmail.com"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node _modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "plop": "plop --plopfile generators/plopfile.js",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/migrations/entities -d ./src/data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "generate:models": "typeorm-model-generator -h localhost -d PDI -u pdi -x 1@asdfgPDI -e mssql -o ./src/database",
    "db:backup": "echo \"BACKUP DATABASE PDI TO DISK = 'pdi_backup.bak' | npx mssql --server localhost --database PDI --user sa --password 1@asdfgsa"
  },
  "dependencies": {
    "@nestjs/apollo": "10.0.11",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/graphql": "10.0.11",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/typeorm": "8.0.3",
    "apollo-server-express": "3.7.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "date-fns": "2.28.0",
    "graphql": "16.5.0",
    "ldapts": "3.2.4",
    "mssql": "8.1.0",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "plop": "3.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "typeorm": "0.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "6.3.1",
    "@nestjs/cli": "8.0.0",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.0.0",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/pluralize": "0.0.29",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.2.5",
    "pluralize": "8.0.0",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.0.0",
    "typeorm-model-generator": "0.4.6",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(resolver|service).(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@data-source": "<rootDir>/data-source.ts",
      "^@appModule": "<rootDir>/app.module.ts",
      "^@constants/(.*)$": "<rootDir>/constants/$1",
      "^@guards/(.*)$": "<rootDir>/guards/$1",
      "^@middlewares/(.*)$": "<rootDir>/middlewares/$1",
      "^@strategies/(.*)$": "<rootDir>/strategies/$1",
      "^@ldap/(.*)$": "<rootDir>/resources/ldap/$1",
      "^@users/(.*)$": "<rootDir>/resources/users/$1",
      "^@users-info/(.*)$": "<rootDir>/resources/users-info/$1",
      "^@departments/(.*)$": "<rootDir>/resources/departments/$1",
      "^@departments-i18n/(.*)$": "<rootDir>/resources/departments-i18n/$1",
      "^@sections/(.*)$": "<rootDir>/resources/sections/$1",
      "^@sections-i18n/(.*)$": "<rootDir>/resources/sections-i18n/$1",
      "^@questions/(.*)$": "<rootDir>/resources/questions/$1",
      "^@questions-i18n/(.*)$": "<rootDir>/resources/questions-i18n/$1",
      "^@skills/(.*)$": "<rootDir>/resources/skills/$1",
      "^@skills-i18n/(.*)$": "<rootDir>/resources/skills-i18n/$1",
      "^@feedbacks/(.*)$": "<rootDir>/resources/feedbacks/$1",
      "^@feedbacks-i18n/(.*)$": "<rootDir>/resources/feedbacks-i18n/$1",
      "^@evaluations/(.*)$": "<rootDir>/resources/evaluations/$1",
      "^@evaluations-i18n/(.*)$": "<rootDir>/resources/evaluations-i18n/$1",
      "^@goals/(.*)$": "<rootDir>/resources/goals/$1",
      "^@kpis/(.*)$": "<rootDir>/resources/kpis/$1",
      "^@evaluation-goals/(.*)$": "<rootDir>/resources/evaluation-goals/$1",
      "^@evaluation-goals-kpis/(.*)$": "<rootDir>/resources/evaluation-goals-kpis/$1",
      "^@pdi-coachings/(.*)$": "<rootDir>/resources/pdi-coachings/$1",
      "^@pdi-qualities/(.*)$": "<rootDir>/resources/pdi-qualities/$1",
      "^@pdi-competences-categories/(.*)$": "<rootDir>/resources/pdi-competences-categories/$1",
      "^@pdi-competences-categories-i18n/(.*)$": "<rootDir>/resources/pdi-competences-categories-i18n/$1",
      "^@ratings/(.*)$": "<rootDir>/resources/ratings/$1",
      "^@ratings-i18n/(.*)$": "<rootDir>/resources/ratings-i18n/$1",
      "^@performed-evaluations/(.*)$": "<rootDir>/resources/performed-evaluations/$1",
      "^@performed-questions/(.*)$": "<rootDir>/resources/performed-questions/$1",
      "^@performed-skills/(.*)$": "<rootDir>/resources/performed-skills/$1",
      "^@performed-goals/(.*)$": "<rootDir>/resources/performed-goals/$1",
      "^@performed-goals-kpis/(.*)$": "<rootDir>/resources/performed-goals-kpis/$1",
      "^@performed-feedbacks/(.*)$": "<rootDir>/resources/performed-feedbacks/$1",
      "^@pages/(.*)$": "<rootDir>/resources/pages/$1",
      "^@medias/(.*)$": "<rootDir>/resources/media/$1",
      "^@components/(.*)$": "<rootDir>/resources/components/$1"
    }
  }
}
